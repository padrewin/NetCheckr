<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <!-- Aplicația helper rulează exclusiv în fundal -->
    <key>LSBackgroundOnly</key>
    <true/>

    <!-- Identificatorul unic pentru helper -->
    <key>CFBundleIdentifier</key>
    <string>dev.padrewin.LaunchAtLoginHelper</string>

    <!-- Numele și versiunea helper-ului -->
    <key>CFBundleName</key>
    <string>LaunchAtLoginHelper</string>
    <key>CFBundleDisplayName</key>
    <string>LaunchAtLoginHelper</string>
    <key>CFBundleVersion</key>
    <string>1.0</string>
    <key>CFBundleShortVersionString</key>
    <string>1.0</string>

    <!-- Aplicația helper nu afișează ferestre -->
    <key>NSHighResolutionCapable</key>
    <true/>
</dict>
</plist>